<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd
    http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.6.xsd ">

    <changeSet id="before_insert_data_table.STUDY_SETTINGS" author="ugryumov.alexey" context="postgresql" labels="insert_data_study_settings_table">
        <tagDatabase tag="before_insert_data_study_settings_table"/>
    </changeSet>

    <changeSet id="insert_data_table.STUDY_SETTINGS" author="ugryumov.alexey" context="postgresql" labels="insert_data_study_settings_table">

        <insert tableName="STUDY_SETTINGS">
            <column name="USER_ID" value="2"></column>
            <column name="VOCABULARY_ID" value="2"></column>
            <column name="MIN_BREAK_PERIOD" value="60"></column>
            <column name="CORRECT_ATTEMPTS_REQUIRED" value="10"></column>
        </insert>

    </changeSet>

</databaseChangeLog>
